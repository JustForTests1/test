   CLCROOT1.OBJ_NIK            = '';
   CLCROOT1.DUMP_BILINFO       = '0'B;
   CLCROOT1.IN_ERROR           = '0'B;
   CLCROOT1.INTERNAL_ERROR     = '0'B;
   CLCROOT1.ERROR_NUMBER       = '';
   CLCROOT1.ERROR_VARIABLE     = '';
   CLCROOT1.ERROR_SEVERITY     = '';
   CLCROOT1.ERROR_SEVERITY_DEF = '';
   /* CLCROOT INITIALIZED */
   DCL 1 CNV1 STATIC, %INCLUDE QCHCNVS;;
   CNVP1 = ADDR (CNV1);
   CNV1.REF_HWX_BASE          = NULL;
   CNV1.D_WARREND_BOX         = '';
   CNV1.REF_HWX_COMP          = NULL;
