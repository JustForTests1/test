   SLPROOT1.COLL_HWC_FEA       = NULL;
   SLPROOT1.COLL_PRC           = NULL;
   SLPROOT1.COLL_CSL           = NULL;

   SLPROOT1.TY_ORDERHANDLING   = '';

   SLPROOT1.KEY_FOR_PRC.ID_SLV = 0;
   SLPROOT1.KEY_FOR_PRC.TYPE   = '';
   SLPROOT1.KEY_FOR_PRC.MDLFEA = '';
   SLPROOT1.KEY_FOR_PRC.D_FR   = MIN_DB2_DAT();
   SLPROOT1.KEY_FOR_PRC.D_TO   = MAX_DB2_DAT();
   SLPROOT1.KEY_FOR_PRC.D_PRCREF = MIN_DB2_DAT();

   SLPROOT1.KEY_FOR_PRC_PREV.ID_SLV = 0;
   SLPROOT1.KEY_FOR_PRC_PREV.TYPE   = '';
   SLPROOT1.KEY_FOR_PRC_PREV.MDLFEA = '';
   SLPROOT1.KEY_FOR_PRC_PREV.D_FR   = MIN_DB2_DAT();
   SLPROOT1.KEY_FOR_PRC_PREV.D_TO   = MAX_DB2_DAT();
   SLPROOT1.KEY_FOR_PRC_PREV.D_PRCREF = MIN_DB2_DAT();
   /* SLPROOT INITIALIZED */

   /*---------------------------------------------------------------*/
   /* SL_PRC STRUCTURE FOR SERVICE LEVEL PRICING
   /*---------------------------------------------------------------*/
   DCL 1 SL_PRC STATIC, %INCLUDE QCHSLPRC ;;
