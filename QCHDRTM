   DBIL1.D_EFF       = '0001-01-01';
   DBIL1.D_EXP       = '9999-12-31';
   DBIL1.IS_TOUCHED  = '0'B;
   DBIL1.REF_SOX     = NULL;
   DBIL1.REF_SOC     = NULL;
