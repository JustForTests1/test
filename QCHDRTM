   DBIL1.ARR_LIO(2).REF_FSC = NULL;
   DBIL1.ARR_LIO(3).REF_LIO = NULL;
   DBIL1.ARR_LIO(3).REF_FSC = NULL;
   DBIL1.ARR_LIO(4).REF_LIO = NULL;
   DBIL1.ARR_LIO(4).REF_FSC = NULL;
   DBIL1.ARR_LIO(5).REF_LIO = NULL;
   DBIL1.ARR_LIO(5).REF_FSC = NULL;

