         5 COLL_PPP     POINTER INIT(NULL);
   BIDROOT = ADDR(SBROOT);

   /*---------------------------------------------------------------*/
   /* STRUCTURE DEFINITIONS FOR FOREIGN APPLICATIONS                */
   /*---------------------------------------------------------------*/
   /*--- PEW */
   DCL 1 PEWINFO STATIC   %INCLUDE  QCHPEWRF;;
