   DCL 1 UDD1 STATIC, %INCLUDE QCHUDDS;; UDDP1 = ADDR (UDD1); UDD1='';

   /* WORK STRUCTURES */
   /* MSO 2001-01-10:
   /* MOVED XX5 FROM QCHDSTM TO QCHDRTM BECAUSE QCHDSTM PROGRAM TOO */
   /* LARGE.                                                        */
   DCL 1 XX5  STATIC, %INCLUDE QCHXX5S;; XX5P1 = ADDR (XX5 );
                                                      %INCLUDE QCHXX5I;
   /* GHA 2001-02-18: */
   /* PROGRAM TOO LARGE -> FOLLOWING STRUCTURES MOVE FROM DSTM      */
   /* GHA BEGIN      */
   DCL 1 CDT1 STATIC, %INCLUDE QCHCDTSM;;CDTP1 = ADDR (CDT1); CDT1='';
   DCL 1 CMT1 STATIC, %INCLUDE QCHCMTS;; CMTP1 = ADDR (CMT1); CMT1='';
