 /*********************************************************************/
 /* MODULE NAME  : QCHDRTM                                            */
 /* APPLICATION  : CHIS                (C) COPYRIGHT IBM CORP. 1991   */
 /*-------------------------------------------------------------------*/
 /* FUNCTION     : DO "ONCE PER ENTER" TABLE STRUCTURE ALLOC+INIT     */
 /* >>>>>>>>     : CALLED FROM MSSM,SPAM ( AFTER EVERY ENTER )        */
 /* >>>>>>>>     : CALLED RECURSIVELY VIA DDDM                        */
 /* AUTHOR       : F. GROSSENBERGER                                   */
 /* DATE         : 13-03-1987                                         */
 /* HISTORY      : TYPE HIST IN COMMAND LINE AND PRESS ENTER          */
 /* $P1=DBCSPRT,6.2,06/23/98,DFE: NEW NLS-TRANSLATED-TEXT-TABLE NLT   */
 
 /*********************************************************************/
   %INCLUDE QCH##PAD;
   %INCLUDE QCH##EPS;
 QCHDRTM : PROC ($$USPAPTR) REORDER;
   #MODIDENT(QCHDRTM);
   DFT  RANGE(*) STATIC;

   %INCLUDE QCHDEPS;

   DCL  $$USPAPTR POINTER;
   DCL  $XTVTAB   POINTER            EXTERNAL        ;
   DCL 1 SP BASED ($$USPAPTR), %INCLUDE QCHSPAS ; ;

 /*-------------------------------------------------------------------*/
 /****   DECLARATIONS OF PARAMETERS PASSED AND ASSOCIATED AREAS   ****/
 /*-------------------------------------------------------------------*/

   DCL  (NULL,ADDR)  BUILTIN;

 /*-------------------------------------------------------------------*/
 /* PROGRAM LOGIC STARTS                                              */
 /* PUT A TRACE TABLE ENTRY                                           */
 /*-------------------------------------------------------------------*/

   CALL QCHDTTM ('QCHDRTM ', SP.RUN_MODE);

   DCL 1 SPP BASED(SP.PTRP1),      %INCLUDE QCHPTRS; ;
   DCL 1 SP2 BASED(SPP.PTRP2),     %INCLUDE QCHPTR2; ;

 /*-------------------------------------------------------------------*/
 /* INITIALIZE HELP POINTER FOR OBJECTCHAINLOOP                       */
 /*-------------------------------------------------------------------*/
   SPP.X$XP1 = NULL;

 /*-------------------------------------------------------------------*/
 /* STRUCTURE FOR POINTERS TO STRUCTURES OF FOREIGN APPLICATIONS      */
 /*-------------------------------------------------------------------*/
   DCL 1 FAPS STATIC, %INCLUDE QCHPFAP;;
   FOREIGNAPPLS = ADDR (FAPS);
   FAPS=NULL;

 /*-------------------------------------------------------------------*/
 /* ACCENT OBJECT POINTER STRUCTURE                                   */
 /*-------------------------------------------------------------------*/
   DCL 1 CLAS STATIC, %INCLUDE QCHPOBJ;; CLASS = ADDR (CLAS); CLAS=NULL;

 /*-------------------------------------------------------------------*/
 /* PARAMETER PASSING STRUCTURE FOR CFLS                              */
 /*-------------------------------------------------------------------*/
   DCL 1 COMS STATIC, %INCLUDE QCHCOMS;; COMP1 = ADDR (COMS); COMS='';
   DCL 1 CMMS STATIC, %INCLUDE QCHCMMS;; CMMP1 = ADDR (CMMS);
         CMMS.COMMON = '';
 /*DCL 1 BMSS STATIC, %INCLUDE QCHBMSS;;                        OBS810*/
 /*                           BMSP1 = ADDR (BMSS); BMSS='';     OBS810*/

   SPP.PROCESS = NULL;
   SPP.CURPROC = NULL;

 /* DBCS DECLARES BEGIN -------------------------------------------*/
   DCL 1 NLC1 STATIC, %INCLUDE QCHNLCS;; NLCP1 = ADDR (NLC1); NLC1='';
   DCL 1 NCT1 STATIC, %INCLUDE QCHNCTS;; NCTP1 = ADDR (NCT1); NCT1='';
   DCL 1 NLS1 STATIC, %INCLUDE QCHNLSS;; NLSP1 = ADDR (NLS1); NLS1='';
   DCL 1 NLT1 STATIC, %INCLUDE QCHNLTS;; NLTP1 = ADDR (NLT1); NLT1='';
 /* DBCS DECLARES END   -------------------------------------------*/

 /* NEW TABLE DECLARES  -------------------------------------------*/
   DCL 1 BOR1 STATIC, %INCLUDE QCHBORS;; BORP1 = ADDR (BOR1); BOR1='';
   DCL 1 GFD1 STATIC, %INCLUDE QCHGFDS;; GFDP1 = ADDR (GFD1); GFD1='';
   DCL 1 GFO1 STATIC, %INCLUDE QCHGFOS;; GFOP1 = ADDR (GFO1); GFO1='';
   DCL 1 PPR1 STATIC, %INCLUDE QCHPPRS;; PPRP1 = ADDR (PPR1); PPR1='';
   DCL 1 PRO1 STATIC, %INCLUDE QCHPROS;; PROP1 = ADDR (PRO1); PRO1='';
   DCL 1 TCT1 STATIC, %INCLUDE QCHTCTS;; TCTP1 = ADDR (TCT1); TCT1='';
   DCL 1 TFC1 STATIC, %INCLUDE QCHTFCS;; TFCP1 = ADDR (TFC1); TFC1='';
   DCL 1 TFD1 STATIC, %INCLUDE QCHTFDS;; TFDP1 = ADDR (TFD1); TFD1='';
   DCL 1 TWW1 STATIC, %INCLUDE QCHTWWS;; TWWP1 = ADDR (TWW1); TWW1='';
   DCL 1 CWS1 STATIC, %INCLUDE QCHCWSS;; CWSP1 = ADDR (CWS1); CWS1='';
   DCL 1 UCL1 STATIC, %INCLUDE QCHUCLS;; UCLP1 = ADDR (UCL1); UCL1='';
   DCL 1 UPF1 STATIC, %INCLUDE QCHUPFS;; UPFP1 = ADDR (UPF1); UPF1='';
   DCL 1 PRS1 STATIC, %INCLUDE QCHPRSS;; PRSP1 = ADDR (PRS1); PRS1='';
   DCL 1 CFO1 STATIC, %INCLUDE QCHCFOS;; CFOP1 = ADDR (CFO1); CFO1='';
   DCL 1 UIH1 STATIC, %INCLUDE QCHUIHS;; UIHP1 = ADDR (UIH1); UIH1='';
   DCL 1 HLA1 STATIC, %INCLUDE QCHHLAS;; HLAP1 = ADDR (HLA1); HLA1='';
   DCL 1 HLC1 STATIC, %INCLUDE QCHHLCS;; HLCP1 = ADDR (HLC1); HLC1='';
   DCL 1 DCA1 STATIC, %INCLUDE QCHDCAS;; DCAP1 = ADDR (DCA1); DCA1='';
   DCL 1 DCF1 STATIC, %INCLUDE QCHDCFS;; DCFP1 = ADDR (DCF1); DCF1='';
   DCL 1 DFG1 STATIC, %INCLUDE QCHDFGS;; DFGP1 = ADDR (DFG1); DFG1='';
   DCL 1 DIA1 STATIC, %INCLUDE QCHDIAS;; DIAP1 = ADDR (DIA1); DIA1='';
   DCL 1 DPE1 STATIC, %INCLUDE QCHDPES;; DPEP1 = ADDR (DPE1); DPE1='';
   DCL 1 DPO1 STATIC, %INCLUDE QCHDPOS;; DPOP1 = ADDR (DPO1); DPO1='';
   DCL 1 DSG1 STATIC, %INCLUDE QCHDSGS;; DSGP1 = ADDR (DSG1); DSG1='';
   DCL 1 DTE1 STATIC, %INCLUDE QCHDTES;; DTEP1 = ADDR (DTE1); DTE1='';
   DCL 1 DWW1 STATIC, %INCLUDE QCHDWWS;; DWWP1 = ADDR (DWW1); DWW1='';
   DCL 1 DFP1 STATIC, %INCLUDE QCHDFPS;; DFPP1 = ADDR (DFP1); DFP1='';
   DCL 1 PPP1 STATIC, %INCLUDE QCHPPPS;; PPPP1 = ADDR (PPP1); PPP1='';
   DCL 1 DCE1 STATIC, %INCLUDE QCHDCES;; DCEP1 = ADDR (DCE1); DCE1='';

   DCL 1 BAH1 STATIC, %INCLUDE QCHBAHS;; BAHP1 = ADDR (BAH1); BAH1='';
   DCL 1 BCR1 STATIC, %INCLUDE QCHBCRS;; BCRP1 = ADDR (BCR1); BCR1='';
   DCL 1 COC1 STATIC, %INCLUDE QCHCOCS;; COCP1 = ADDR (COC1); COC1='';
   DCL 1 PCR1 STATIC, %INCLUDE QCHPCRS;; PCRP1 = ADDR (PCR1); PCR1='';
   DCL 1 PRV1 STATIC, %INCLUDE QCHPRVS;; PRVP1 = ADDR (PRV1); PRV1='';
   DCL 1 STM1 STATIC, %INCLUDE QCHSTMS;; STMP1 = ADDR (STM1); STM1='';

   DCL 1 AMNT1 STATIC, %INCLUDE QCHOAMNT;;                     /* GWI */
                      AMNTP1 = ADDR(AMNT1);                    /* GWI */
                      AMNT1.PTRFIRST = NULL();                 /* GWI */

   /* IR701008 */
   DCL 1 MAD1 STATIC, %INCLUDE QCHMADS;; MADP1 = ADDR (MAD1); MAD1='';
   /* IR701022 */
   DCL 1 BMC1 STATIC, %INCLUDE QCHBMCS;; BMCP1 = ADDR (BMC1); BMC1='';
   DCL 1 BMM1 STATIC, %INCLUDE QCHBMMS;; BMMP1 = ADDR (BMM1); BMM1='';
   DCL 1 BPC1 STATIC, %INCLUDE QCHBPCS;; BPCP1 = ADDR (BPC1); BPC1='';
