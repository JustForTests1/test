   DELI1.TY_REF_ELEM2 = '';
   DELI1.REF_LIO_2    = NULL;
   DELI1.REF_FSC_2    = NULL;
   DELI1.Q_FROM       = 0;
   /*-GST*/

   /*---------------------------------------------------------------*/
   /* ROOT OF SERVICE LEVEL PRICING OBJECT MODEL
   /*---------------------------------------------------------------*/
   DCL 1 SLPROOT1 STATIC, %INCLUDE QCHRSLS ;;
